         <string notr="true">/* 整体容器背景 */
QTabWidget::pane {
    border: 1px solid #2e3b55;
    background-color: #1a1f2c;
    top: -1px;  /* 防止 border 被切断 */
}

/* 标签栏区域背景 */
QTabBar {
    background-color: #1a1f2c;
}

/* 普通标签样式 */
QTabBar::tab {
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #3a4a68,
        stop: 0.5 #2e3b55,
        stop: 1 #1d273a
    );
    color: white;
    border: 1px solid #0f1b2e;
    border-bottom-color: #2e3b55; /* 合并边框 */
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    padding: 6px 12px;
    min-width: 80px;
}

/* 选中的标签页 */
QTabBar::tab:selected {
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #44597f,
        stop: 0.5 #344765,
        stop: 1 #1e2d44
    );
    border: 1px solid #3c4a64;
    border-bottom: 1px solid #1a1f2c; /* 与内容区背景融合 */
    font-weight: bold;
}

/* 鼠标悬停 */
QTabBar::tab:hover {
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #4e6086,
        stop: 1 #2e3a55
    );
}
