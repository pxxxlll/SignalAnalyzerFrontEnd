/* blue metal */
QPushButton {
    color: white;
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #3a4a68,
        stop: 0.5 #2e3b55,
        stop: 1 #1d273a
    );
    border: 1px solid #0f1b2e;
    border-radius: 6px;
    padding: 6px 12px;
    font-weight: bold;
    font-size: 14px;
    text-align: center;
    /*box-shadow: 0px 2px 5px rgba(0,0,0,0.6);*/
}

QPushButton:hover {
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #44597f,
        stop: 0.5 #344765,
        stop: 1 #1e2d44
    );
    border: 1px solid #2c3c5a;
}

QPushButton:pressed {
    background-color: qlineargradient(
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0 #1e2a3d,
        stop: 1 #0f1725
    );
    border: 1px solid #000000;
    padding-top: 7px;  /* Pressed visual effect */
    padding-bottom: 5px;
}

QPushButton:disabled {
    background-color: #444a5a;
    color: #999;
    border: 1px solid #333;
}

